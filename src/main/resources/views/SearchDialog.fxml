<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <Pane prefHeight="53.0" prefWidth="342.0" BorderPane.alignment="CENTER">
            <TextField layoutX="89.0" layoutY="14.0" prefHeight="25.0" prefWidth="240.0"/>
            <Label layoutX="14.0" layoutY="18.0" text="Search Text:"/>
        </Pane>
    </center>
    <bottom>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="search_button" mnemonicParsing="false" text="Search"/>
                <Button fx:id="cancel_button" mnemonicParsing="false" text="Cancel"/>
            </buttons>
        </ButtonBar>
    </bottom>
</BorderPane>
